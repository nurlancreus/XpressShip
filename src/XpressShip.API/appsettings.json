{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "Default": "Server=.;Database=XpressShipDB;Integrated Security=true;TrustServerCertificate=true;"
  },
  "Token": {
    "Access": {
      "Audience": "https://localhost:3000/",
      "Issuer": "https://localhost:7223/",
      "SecurityKey": "12345678-abcd-efgh-ijkl-9876543210mn",
      "AccessTokenLifeTimeInMinutes": 60
    },
    "Refresh": {
      "RefreshTokenLifeTimeInMinutes": 30
    }
  },
  "Caching": {
    "InMemory": {
      "CountryData": {
        "SlidingExpirationInHours": 1,
        "AbsoluteExpirationInHours": 6,
        "CacheKey": "Countries"
      }
    }
  },
  "API": {
    "GeoCodeAPI": {
      "BaseUrl": "https://geocodeapi.com/",
      "Version": "v1",
      "ApiKey": "test-api-key-123456",
      "ApiSecret": "test-api-secret"
    }
  },
  "EmailConfiguration": {
    "From": "test-email@example.com",
    "SmtpServer": "smtp.testemail.com",
    "Port": 587,
    "Username": "test-email@example.com",
    "Password": "test-email-password"
  },
  "PaymentGateways": {
    "Stripe": {
      "SecretKey": "sk_test_test1234567890abcdefg",
      "WebhookSecret": "test-webhook-secret"
    }
  },
  "AllowedHosts": "*"
}
